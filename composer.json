{
    "name": "core23/lastfm-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides services for using the last.fm API inside the symfony.",
    "keywords": [
        "symfony",
        "bundle",
        "last.fm",
        "api",
        "crawler",
        "webservice client"
    ],
    "homepage": "https://core23.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Gripp",
            "email": "mail@core23.de"
        }
    ],
    "require": {
        "php": "^7.2",
        "core23/lastfm-api": "^2.0 || dev-master",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/config": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/event-dispatcher-contracts": "^1.1",
        "symfony/expression-language": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-foundation": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/routing": "^4.2",
        "symfony/twig-bridge": "^4.2",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.3",
        "localheinz/composer-normalize": "^1.3",
        "nyholm/psr7": "^1.0",
        "symfony/http-client": "^4.4 || ^5.0"
    },
    "suggest": {
        "cache/adapter-bundle": "Symfony cache adapter",
        "symfony/http-client": "Symfony HTTP client implementation"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Core23\\LastFmBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Core23\\LastFmBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ]
    }
}
