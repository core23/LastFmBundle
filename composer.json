{
  "name": "core23/lastfm-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides services for using the last.fm API inside the symfony.",
  "keywords": [
    "symfony",
    "bundle",
    "last.fm",
    "api",
    "crawler",
    "webservice client"
  ],
  "homepage": "https://core23.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Gripp",
      "email": "mail@core23.de"
    }
  ],
  "require": {
    "php": "^7.1",
    "core23/lastfm-api": "^0.2",
    "php-http/httplug-bundle": "^1.1",
    "symfony/framework-bundle": "^2.8.6 || ^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "matthiasnoback/symfony-dependency-injection-test": "^1.1",
    "php-http/mock-client": "^0.3",
    "phpunit/phpunit": "^5.7",
    "sebastian/exporter": "^2.0",
    "sllh/php-cs-fixer-styleci-bridge": "^2.0",
    "symfony/phpunit-bridge": "^3.2"
  },
  "suggest": {
    "php-http/buzz-adapter": "Buzz HTTP client implementation",
    "php-http/guzzle6-adapter": "Guzzle HTTP client implementation",
    "cache/adapter-bundle": "Symfony cache adapter"
  },
  "autoload": {
    "psr-4": {
      "Core23\\LastFmBundle\\": ""
    },
    "exclude-from-classmap": [
        "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
