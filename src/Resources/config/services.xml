<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="Nucleos\LastFmBundle\Session\SessionManagerInterface" class="Nucleos\LastFmBundle\Session\SessionManager">
      <argument type="service" id="session"/>
    </service>
    <service id="Nucleos\LastFm\Connection\PsrClientConnection">
      <argument type="service" id="nucleos_lastfm.http.client"/>
      <argument type="service" id="nucleos_lastfm.http.message_factory"/>
      <argument>%nucleos_lastfm.api.endpoint%</argument>
    </service>
    <service id="Nucleos\LastFm\Connection\ConnectionInterface" alias="Nucleos\LastFm\Connection\PsrClientConnection"/>
    <service id="Nucleos\LastFm\Client\ApiClientInterface" class="Nucleos\LastFm\Client\ApiClient">
      <argument type="service" id="Nucleos\LastFm\Connection\ConnectionInterface"/>
      <argument>%nucleos_lastfm.api.app_id%</argument>
      <argument>%nucleos_lastfm.api.shared_secret%</argument>
    </service>
    <service id="Nucleos\LastFm\Crawler\ArtistEventCrawlerInterface" class="Nucleos\LastFm\Crawler\ArtistEventCrawler">
      <argument type="service" id="Nucleos\LastFm\Connection\ConnectionInterface"/>
    </service>
    <service id="Nucleos\LastFm\Crawler\EventInfoCrawlerInterface" class="Nucleos\LastFm\Crawler\EventInfoCrawler">
      <argument type="service" id="Nucleos\LastFm\Connection\ConnectionInterface"/>
    </service>
    <service id="Nucleos\LastFm\Crawler\EventListCrawlerInterface" class="Nucleos\LastFm\Crawler\EventListCrawler">
      <argument type="service" id="Nucleos\LastFm\Connection\ConnectionInterface"/>
    </service>
    <service id="Nucleos\LastFm\Crawler\UserEventCrawlerInterface" class="Nucleos\LastFm\Crawler\UserEventCrawler">
      <argument type="service" id="Nucleos\LastFm\Connection\ConnectionInterface"/>
    </service>
    <service id="Nucleos\LastFm\Service\AlbumServiceInterface" class="Nucleos\LastFm\Service\AlbumService">
      <argument type="service" id="Nucleos\LastFm\Client\ApiClientInterface"/>
    </service>
    <service id="Nucleos\LastFm\Service\ArtistServiceInterface" class="Nucleos\LastFm\Service\ArtistService">
      <argument type="service" id="Nucleos\LastFm\Client\ApiClientInterface"/>
    </service>
    <service id="Nucleos\LastFm\Service\AuthServiceInterface" class="Nucleos\LastFm\Service\AuthService">
      <argument type="service" id="Nucleos\LastFm\Client\ApiClientInterface"/>
      <argument>%nucleos_lastfm.api.auth_url%</argument>
    </service>
    <service id="Nucleos\LastFm\Service\ChartServiceInterface" class="Nucleos\LastFm\Service\ChartService">
      <argument type="service" id="Nucleos\LastFm\Client\ApiClientInterface"/>
    </service>
    <service id="Nucleos\LastFm\Service\GeoServiceInterface" class="Nucleos\LastFm\Service\GeoService">
      <argument type="service" id="Nucleos\LastFm\Client\ApiClientInterface"/>
    </service>
    <service id="Nucleos\LastFm\Service\LibraryServiceInterface" class="Nucleos\LastFm\Service\LibraryService">
      <argument type="service" id="Nucleos\LastFm\Client\ApiClientInterface"/>
    </service>
    <service id="Nucleos\LastFm\Service\TagServiceInterface" class="Nucleos\LastFm\Service\TagService">
      <argument type="service" id="Nucleos\LastFm\Client\ApiClientInterface"/>
    </service>
    <service id="Nucleos\LastFm\Service\TrackServiceInterface" class="Nucleos\LastFm\Service\TrackService">
      <argument type="service" id="Nucleos\LastFm\Client\ApiClientInterface"/>
    </service>
    <service id="Nucleos\LastFm\Service\UserServiceInterface" class="Nucleos\LastFm\Service\UserService">
      <argument type="service" id="Nucleos\LastFm\Client\ApiClientInterface"/>
    </service>
  </services>
</container>
